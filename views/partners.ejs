<%- include('./partials/head') %>
<%- include('./partials/header') %>

<main class="list">
    <div class="toolboard__intro">
        <h1>Websites</h1>
    </div>
    <div class="websites">
        <% urls.forEach(url=> { %>
        <div class="websites__container">
            <div>
                <h4><%= url.website.titel %></h4>
                <a class="websites__link" target="_blank" href="<%= url.url %>"><%= url.url %></a>
            </div>
            <div class="website__urls">
                <a class="website__icon" href="#"><img src="/assets/checkmark-outline.svg" alt=""></a>
            </div>
        </div>
        <%})%>
    </div>
</main>

<%- include('./partials/foot') %>